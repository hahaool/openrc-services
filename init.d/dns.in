#!@RUNSCRIPT@

depend(){
   after network net
}

start() {
   ebegin "Configuring Systemd Dns"
   for n in {101.6.6.6,9.9.9.9,223.5.5.5}; do
       echo "nameserver ${n}" >>/run/systemd/resolve/stub-resolv.conf
    done
  }

start_pre() {
    ebegin "Checking directory /run/systemd/resolve"
 	checkpath -d /run/systemd/resolve -q
}
