#!@RUNSCRIPT@

description="GNOME Display Manager"

supervisor=supervise-daemon
command=/usr/bin/gdm

depends() 
{
	provide xdm display-manager
	needs localmount
	
	after bootmisc consolefont modules netmount
        after keymaps
        want logind
}

start_pre() {
	checkpath -d -o gdm:gdm -m755 /var/lib/gdm
}

start() {
	ebegin "$description"
	eend
}
